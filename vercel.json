{
  "version": 2,
  "builds": [
    { "src": "dontg-api/dist/main.js", "use": "@vercel/node" },
    {
      "src": "dontg-api/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "dontg-web/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dontg-api/dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
